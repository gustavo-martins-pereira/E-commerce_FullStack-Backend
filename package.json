{
    "name": "ecommerce-fullstack-backend",
    "version": "1.0.0",
    "description": "Backend for the \"E-Commerce FullStack\" project",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start:dev": "nodemon src/server.js",
        "start:prod": "node src/server.js",
        "migrate": "npx sequelize-cli db:migrate",
        "seed": "npx sequelize-cli db:seed:all",
        "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
        "seed:undo": "npx sequelize-cli db:seed:undo:all",
        "setup-db": "npm run migrate && npm run seed"
    },
    "keywords": [
        "postgresql",
        "express"
    ],
    "author": "Gustavo Martins Pereira",
    "license": "UNLICENSED",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.797.0",
        "@aws-sdk/s3-request-presigner": "^3.797.0",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-validator": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "jwt-decode": "^4.0.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.12.0",
        "pg-hstore": "^2.3.4",
        "redis": "^4.7.0",
        "sequelize": "^6.37.3",
        "sharp": "^0.34.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "nodemon": "^3.1.4",
        "sequelize-cli": "^6.6.2"
    }
}
